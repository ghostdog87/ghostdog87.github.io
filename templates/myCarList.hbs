<div class="my-listing">
    <p id="listing-title">{{title}}</p>
    <img src="{{imageUrl}}">

    <div class="listing-props">
        <h2>Brand: {{brand}}</h2>
        <h3>Model: {{model}}</h3>
        <h3>Year: {{year}}</h3>
        <h3>Price: {{price}}$</h3>
    </div>
    <div class="my-listing-buttons">
        <a href="#" id="{{_id}}" class="my-button-list detailsID">Details</a>
        <a href="#" id="{{_id}}" class="my-button-list editID">Edit</a>
        <a href="#" id="{{_id}}" class="my-button-list deleteID">Delete</a>
    </div>
</div>